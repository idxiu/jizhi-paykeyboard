<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:height="160dp" android:width="160dp">
        <rotate
            android:fromDegrees="45"
            android:pivotX="135%"
            android:pivotY="15%">
            <shape android:shape="rectangle">
                <size
                    android:width="140dp"
                    android:height="140dp"/>
                <gradient android:startColor="@color/f8_tuijian_1" android:endColor="@color/f8_tuijian_2" android:type="linear"/>
            </shape>
        </rotate>
    </item>

</layer-list>